<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE linx SYSTEM "linx_taskdef_3_1.dtd">
<linx>
	<task name="Manage Contributors" taskVersion="1.0" pg="default"
		svURL="custom" taskClass="default">
		<client clientVersion="1.0">
			<displayItems>
				<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
						To save a new Contributor enter data and then click 'Save'
					</value>
				</displayItem>
				<displayItem itemType="Contributor" label="Contributor"
					widget="textinput" />
				<displayItem itemType="ContactInfo" label="Phone Number"
					widget="textinput" />
				<displayItem itemType="Institute"
					label="Institute or Lab" widget="dropdown" rowCount="3">
					<value>
						(Add new institutes at task 'Manage Settings')
					</value>
					<sql>
						select appValue from vwLinx_AppValues where
						appValueType = 'Institute' order by appValue
					</sql>
				</displayItem>
				<displayItem itemType="Comment" label="Comments"
					widget="textarea" rows="4" columns="40" minLength="300" />
				<displayItem itemType="Save" label=" "
					widget="savebutton" />
					<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
					</value>
				</displayItem>
				<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
					</value>
				</displayItem>
				<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
					</value>
				</displayItem>
				<displayItem itemType="Contributors"
					label="Existing Contributors" widget="rowsets_sort" rowCount="10" />
			</displayItems>
		</client>
		<server>
			<items>
				<item itemType="Contributor" type="container"
					new="true">
					<allowQueue>Active Contributors</allowQueue>
					<queueTo>Active Contributors</queueTo>
				</item>
				<item itemType="ContactInfo" type="string">
					<addAsData appliesTo="Contributor"
						dataType="ContactInfo" coordinate="Phone Number" />
				</item>
				<item itemType="Institute" type="string">
					<addAsData appliesTo="Contributor"
						dataType="ContactInfo" coordinate="Institute" />
				</item>
				<item itemType="Comment" type="comment"
					optional="true" commentType="CONTRIBUTOR_COMMENTTYPE" 
					appliesTo="Contributor" >
				</item>
			</items>
		</server>
	</task>
	<task name="Manage Projects" taskVersion="1.0" pg="default"
		svURL="custom" taskClass="default">
		<client clientVersion="1.0">
			<displayItems>
				<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
						To save a new Project enter data and then click 'Save'
					</value>
				</displayItem>
				<displayItem itemType="Project" label="Project Name"
					widget="textinput" uppercase="true" />
				<displayItem itemType="ProjectType" label="Project Type"
					widget="dropdown" rowCount="4">
					<values>
						<value selected="true">(Select)</value>
						<value>Fee for Service</value>
						<value>Collaboration</value>
						<value>Internal</value>
					</values>
				</displayItem>
				<displayItem itemType="ProjectManager"
					label="Project Manager" widget="dropdown" rowCount="3">
					<value>
						(Add new project managers at task 'Manage
						Users')
					</value>
					<sql>
						select username from VWLinx_AccessControl where
						featureName = 'All' and userName not
						in('linx','tjs','wildtype') order by userName
					</sql>
				</displayItem>
				<displayItem itemType="Comment" label="Comments"
					widget="textarea" minLength="300" />
				<displayItem itemType="Save" label=" "
					widget="savebutton" />
				<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
					</value>
				</displayItem>
				<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
					</value>
				</displayItem>
				<displayItem itemType="Placeholder1" label=" "
					widget="label">
					<value>
					</value>
				</displayItem>
				<displayItem itemType="Projects"
					label="Existing Projects" widget="rowsets_sort" rowCount="10" />
			</displayItems>
		</client>
		<server>
			<items>
				<item itemType="Project" type="container" new="true">
					<queueTo>Active Projects</queueTo>
				</item>
				<item itemType="ProjectType" type="string">
					<addAsData appliesTo="Project"
						dataType="Project Type" coordinate="0" />
				</item>
				<item itemType="ProjectManager" type="container"
					new="optional">
					<addAsContent appliesTo="Project"
						contentType="ProjectManager" coordinate="0" />
					<allowQueue>any</allowQueue>
				</item>
				<item itemType="Comment" type="comment"
					optional="true" commentType="PROJECT_COMMENTTYPE"
					appliesTo="Project" >
				</item>
			</items>
		</server>
	</task>
